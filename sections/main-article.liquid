{%- comment -%}
  Comments may not appear right after they are submitted, either to be checked by Shopify's spam filter
  or to await moderation. When a comment is submitted, the browser is redirected to a page
  that includes the new comment id in its URL.
    Example: http://shopname.myshopify.com/blogs/news/2022072-my-post?comment=3721372
{%- endcomment -%}
<!-- sections/main-article.liquid -->
{{ 'bee-section.css' | asset_url | stylesheet_tag }}
{{ 'bee-main-article.css' | asset_url | stylesheet_tag }}
{{ 'bee-button-style.css' | asset_url | stylesheet_tag }}
<link href="{{ 'bee-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
    assign sid = section.id
    assign se_stts = section.settings    
    assign stt_layout = se_stts.layout
    assign stt_image_bg = se_stts.image_bg
    if stt_layout == 'bee-se-container' 
        assign html_layout = '<div class="bee-container">__</div></div>' | split: '__'
    elsif stt_layout == 'bee-container-wrap'
        assign html_layout = '<div class="bee-container">__</div>' | split: '__'
    else
        assign html_layout = '__' | split: '__'
    endif 

    assign article_id = article.id
    assign blog_url = blog.url
    assign image = article.image 
    assign article_tags = article.tags
    assign article_metafields = article.metafields.theme 

    if comment.status == 'pending'
        assign number_of_comments = article.comments_count | plus: 1 
    else 
        assign number_of_comments = article.comments_count 
    endif  
 -%}
<div class="bee-section-inner bee_nt_se_{{ sid }} {{ stt_layout }}{% if stt_image_bg != blank and stt_layout != 'bee-se-container' %} bee-has-imgbg lazyloadbee{% endif %}" {% if stt_image_bg != blank and stt_layout != 'bee-se-container' %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5"{% endif %} {% render 'section_style', se_stts: se_stts -%} >
    {{- html_layout[0] -}}
    {%- if stt_layout == 'bee-se-container' -%}
    <div class="bee-container-inner{% if stt_image_bg != blank %} bee-has-imgbg lazyloadbee{% endif %}" {% if stt_image_bg != blank %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5"{% endif %}>{% endif -%}
        <div class="bee-row bee-rows-col-1">
            {%- if se_stts.feature_image -%}
                <div class="bee-col-item bee-feature-image">
                    <div class="bee-article-image bee-pr bee-bg-11" style="max-width:{{ image.width }}px;margin-left: auto;margin-right: auto;background: url({{ image | image_url: width: 1 }});">                 
                        <img class="lazyloadbee" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg', w: image.width, h: image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}">   
                        {%- if article_metafields.video_url != blank or article_metafields.gallery != blank -%}
                            <div class="bee-article-metafields">
                                {%- if article_metafields.video_url != blank -%}
                                    <div data-open-mfp-iframe class="bee-article-metafields__video" href="{{ article_metafields.video_url }}" data-tooltip="right" title="{{ 'blogs.article.click_video' | t }}">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 512 512"><path d="M443.9,196.9L141.5,10.5C119.6-3,93.1-3.5,70.7,9c-22.4,12.5-35.8,35.3-35.8,61v371.1c0,38.8,31.3,70.6,69.8,70.8c0.1,0,0.2,0,0.3,0c12,0,24.6-3.8,36.3-10.9c9.4-5.7,12.4-18,6.6-27.4c-5.7-9.4-18-12.4-27.4-6.6c-5.5,3.3-10.8,5.1-15.6,5.1 c-14.5-0.1-30.1-12.5-30.1-30.9V70.1c0-11,5.7-20.8,15.4-26.2c9.6-5.4,21-5.1,30.3,0.6l302.4,186.4c9.1,5.6,14.3,14.9,14.3,25.6 c0,10.7-5.3,20-14.4,25.6L204.1,415.9c-9.4,5.8-12.4,18-6.6,27.5c5.8,9.4,18,12.4,27.5,6.6l218.6-133.9 c20.9-12.8,33.5-35,33.5-59.5C477.2,232.1,464.7,209.8,443.9,196.9z"/></svg>
                                    </div>
                                {%- endif -%}
                                {%- if article_metafields.gallery != blank -%}
                                    {%- capture gallery_json %}[{% for image in article_metafields.gallery.value -%}
                                    { "src": {{ image.src | image_url | json }}, "w": {{ image.width }}, "h": {{ image.height }}, "alt": {{ image.alt | json }} }{% unless forloop.last %},{% endunless -%}
                                    {%- endfor %}]{% endcapture -%}
                                    <div class="bee-article-metafields__images" data-pswp-images-trigger='{{ gallery_json }}' data-tooltip="right" title="{{ 'blogs.article.click_gallery' | t }}">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 512 512">
                                            <polygon points="480,480 224,480 224,512 512,512 512,224 480,224"/>
                                            <path d="M448,0H0v448h448V0z M416,416H32v-57.4L134.6,256h50.8l64,64h48.5l64-128H416V416z M416,160h-73.9l-64,128h-15.5l-64-64 h-77.2L32,313.4V32h384V160z"/>
                                            <rect x="480" y="160" width="32" height="32"/>
                                            <rect x="160" y="480" width="32" height="32"/>
                                            <path d="M128,192c35.3,0,64-28.7,64-64s-28.7-64-64-64s-64,28.7-64,64S92.7,192,128,192z M128,96c17.7,0,32,14.3,32,32 s-14.3,32-32,32s-32-14.3-32-32S110.3,96,128,96z"/>
                                        </svg>
                                    </div>
                                {%- endif -%}
                            </div>
                        {%- endif -%}            
                    </div>
                </div>
            {%- endif -%}
            <div class="bee-col-item bee-main-article-page bee-main-area bee-main-area__{{ se_stts.content_width }}">
                {%- for block in section.blocks -%}
                    {%- assign bk_stts = block.settings -%}
                    {%- case block.type -%}
                        {%- when 'content' -%}
                            <div class="bee-article-block bee-row bee-article-content" style="--mgb:{{ bk_stts.mgb }}px;--mgb-tb:{{ bk_stts.mgb_tb }}px;--mgb-mb:{{ bk_stts.mgb_mb }}px;">
                                <article id="bee-article-{{ article_id }}" class=" bee-rte article-{{ article_id }} bee-article-content__{{ bk_stts.dessign_content }} bee-col-item bee-col-12">
                                    {{ article.content | replace: '<blockquote>', '<blockquote class="bee-blockquote"><svg class="bee-icon bee-icon--blockquote" aria-hidden="true" focusable="false" role="presentation"><use href="#bee-icon-blockquote"></use></svg>' }}
                                </article>
                            </div>
                        {%- when 'tags_socials' -%}
                          {% if bk_stts.show_tags or bk_stts.show_socials %}
                            <div class="bee-article-block bee-article-tags-socials" style="--mgb:{{ bk_stts.mgb }}px;--mgb-tb:{{ bk_stts.mgb_tb }}px;--mgb-mb:{{ bk_stts.mgb_mb }}px;">
                              {% if bk_stts.show_tags %}
                                <div class="bee-article-tags">
                                    {%- if article_tags.size > 0 -%}                             
                                        <ul class="bee-article-tags__list bee-article-tags__count-{{ bk_stts.tags_count }}">
                                            {%- for tag in article_tags -%} 
                                              {%- if tag contains 'category_' -%}{%- continue -%}{%- endif -%}
                                                <li class="bee-d-inline-block">
                                                    <a href="{{ blog_url }}/tagged/{{ tag | handle }}">{{ tag | remove :'category_' | remove :'blog_' | strip }}</a>{%- unless forloop.last -%} <span class="bee-seperate"></span> {% endunless -%}
                                                </li>
                                            {%- endfor -%}
                                        </ul>
                                    {%- endif -%}
                                </div>  
                              {%- endif -%}   
                              {% if bk_stts.show_socials %}
                                {% liquid 
                                    assign pri_cl_lightness = bk_stts.pri_cl | color_extract: 'lightness'
                                    assign pri_hover_cl_lightness = bk_stts.pri_hover_cl | color_extract: 'lightness'
                                    if pri_cl_lightness < 85
                                        assign pri_cl2 = "#fff"
                                    else 
                                        assign pri_cl2 = "#000"
                                    endif
                                    if pri_hover_cl_lightness < 85
                                        assign pri_hover_cl2 = "#fff"
                                    else 
                                        assign pri_hover_cl2 = "#000"
                                    endif
                                -%}
                                {{ 'icon-social.css' | asset_url | stylesheet_tag }}
                                <div class="bee-article-socials" style="--pri-cl: {{ bk_stts.pri_cl }};--pri-cl2: {{ pri_cl2 }};--pri-hover-cl: {{ bk_stts.pri_hover_cl }};--pri-hover-cl2: {{ pri_hover_cl2 }};--mgb: 0px;--mgb-tb: 0px;--mgb-mb: 0px; --bd-radius: {{ bk_stts.bd_radius }}px;">
                                    {%- if bk_stts.social_mode == '1' -%}{%- assign follow_social = true -%} {%- endif -%} 
                                    {%- render 'social_sharing', bid: block.id, is_social_dropdown: true, style: bk_stts.social_style, use_color_set: bk_stts.use_color_set, size: bk_stts.social_size, space_h_item: bk_stts.space_h_item, space_h_item_mb: bk_stts.space_h_item_mb, space_v_item: bk_stts.space_v_item, space_v_item_mb: bk_stts.space_v_item_mb, follow_social: follow_social -%} 
                                </div> 
                              {% endif %}
                            </div>
                          {% endif %}                
                        {%- when 'navigation' -%}
                          <div class="bee-article-block bee-article-navigation bee-d-flex bee_{{ bk_stts.image_ratio }} bee_position_{{ bk_stts.image_position }} bee_{{ bk_stts.image_size }}" style="--mgb:{{ bk_stts.mgb }}px;--mgb-tb:{{ bk_stts.mgb_tb }}px;--mgb-mb:{{ bk_stts.mgb_mb }}px;">
                              {%- if blog.previous_article -%}
                                  <article class="bee-article-item bee-article-previous bee-text-start">
                                    <a href="{{ blog.previous_article.url }}" class="bee-article-nav_btn bee-article-navigation__previous">{{ 'blogs.article.previous' | t }}</a>
                                    <div class="bee-article-inner bee-pr bee-oh">
                                      {%- if blog.previous_article.image != blank -%}
                                        <div class="bee-article-thumb bee-pr bee-eff bee-eff-{{ b_effect }} bee-eff-img-{{ img_effect }} ">
                                          <a class="bee-article-img bee-d-block" href="{{ blog_url }}">
                                            <div class="bee_ratio bee-bg-11" style="background: url({{ blog.previous_article.image | image_url: width: 1 }});--aspect-ratioapt: {{ blog.previous_article.image.aspect_ratio | default: 2 }}"  data-cacl-slide>
                                              <img class="lazyloadbee bee-lz--fadeIn bee-obj-eff" data-src="{{ blog.previous_article.image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ blog.previous_article.image.width }}" height="{{ blog.previous_article.image.height }}" alt="{{ blog.previous_article.image.alt | escape }}">
                                            </div>
                                          </a>
                                        </div>
                                      {%- endif -%}
                                      <div class="bee-article-info">
                                        <h3 class="bee-article-title"><a href="{{ blog.previous_article.url }}">{{ blog.previous_article.title }}</a></h3>
                                      </div>
                                    </div>
                                  </article>
                              {% endif -%}
                              {%- if blog.next_article -%}
                                    <article class="bee-article-item bee-article-next bee-text-end">
                                      <a href="{{ blog.next_article.url }}" class="bee-article-nav_btn bee-article-navigation__next">{{ 'blogs.article.next' | t }}</a>
                                      <div class="bee-article-inner bee-pr bee-oh">
                                         <div class="bee-article-info">
                                          <h3 class="bee-article-title"><a href="{{ blog.next_article.url }}">{{ blog.next_article.title }}</a></h3>
                                        </div>
                                        {%- if blog.next_article.image != blank -%}
                                          <div class="bee-article-thumb bee-pr bee-eff bee-eff-{{ b_effect }} bee-eff-img-{{ img_effect }} ">
                                            <a class="bee-article-img bee-d-block" href="{{ blog.next_article.url }}">
                                              <div class="bee_ratio bee-bg-11" style="background: url({{ blog.next_article.image | image_url: width: 1 }});--aspect-ratioapt: {{ blog.next_article.image.aspect_ratio | default: 2 }}"  data-cacl-slide>
                                                <img class="lazyloadbee bee-lz--fadeIn bee-obj-eff" data-src="{{ blog.next_article.image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ blog.next_article.image.width }}" height="{{ blog.next_article.image.height }}" alt="{{ blog.next_article.image.alt | escape }}">
                                              </div>
                                            </a>
                                          </div>
                                        {%- endif -%}
                                      </div>
                                    </article>
                              {%- endif -%}
                          </div>
                        {%- when 'comments' -%}
                            {% unless article.comments_enabled? %}{% continue %}{% endunless -%}
                            <div class="bee-article-block bee-article-comments bee-oh" style="--mgb:{{ bk_stts.mgb }}px;--mgb-tb:{{ bk_stts.mgb_tb }}px;--mgb-mb:{{ bk_stts.mgb_mb }}px;">
                                {%- if number_of_comments > 0 -%}
                                    {%- assign anchorId = '#Comments-' | append: article.id -%}
                                    {%- paginate article.comments by 5 -%}
                                        {%- comment -%}#comments is required, it is used as an anchor link by Shopify.{%- endcomment -%}
                                        <div id="comments">
                                            <h2 id="Comments-{{ article.id }}" class="bee-comments-heading">{% if number_of_comments == 1 %} {{ 'blogs.comments.with_count.one' | t }} {%- else -%}{{ 'blogs.comments.with_count.other' | t: count: number_of_comments }}{%- endif -%}</h2>
                                            <div class="bee-comment__list">
                                                {%- if comment.status == 'pending' and comment.content -%}
                                                    {%- comment -%}If a comment was just submitted with no blank field, show it. {%- endcomment -%}
                                                    <div id="{{ comment.id }}" class="bee-comment__item">{%- render 'comment', comment: comment -%}</div>
                                                {%- endif -%}
                                                {%- for comment in article.comments -%}
                                                    <div id="{{ comment.id }}" class="bee-comment__item">{%- render 'comment', comment: comment -%}</div>
                                                {%- endfor -%}
                                                {%- if paginate.pages > 1 -%}
                                                    <div class="bee-col-12">{%- render 'pagination', paginate: paginate, anchor: anchorId, ajax: 'false' -%}</div>
                                                {%- endif -%}
                                            </div>
                                        </div>
                                    {%- endpaginate -%}
                                {%- endif -%}
                                <div class="bee-line-space"></div>
                                {%- form 'new_comment', article, id: 'CommentForm' -%}
                                    <h2 class="bee-comments-form__heading">{{ 'blogs.comments.title' | t }}</h2>
                                    <p class="bee-comments-form__notes">{{ 'blogs.comments.comments_note_html' | t }}</p>
                                    {%- liquid
                                        assign post_message  = 'blogs.comments.success'
                                        assign blog_moderate = article.moderated?
                                        if blog.moderated? and comment.status == 'pending'
                                            assign post_message = 'blogs.comments.success_moderated'
                                        elsif comment.status == 'unapproved' or comment.status == 'spam'
                                            assign post_message = 'blogs.comments.unapproved'
                                        endif
                                        assign error_heading = 'blogs.comments.error_heading'
                                        assign name          = 'blogs.comments.name'
                                        assign email         = 'blogs.comments.email'
                                        assign message       = 'blogs.comments.message'
                                        assign form_errors   = form.errors
                                        assign form_errors_messages = form_errors.messages
                                        assign isSuccessfully = form.posted_successfully?

                                        render 'form-status', isSuccessfully: isSuccessfully, form_id: 'CommentForm', success_message: post_message, form_errors: form_errors, form_errors_messages: form_errors_messages, error_heading: error_heading, name: name, email: email, message: message 
                                    -%}

                                    {%- if blog_moderate -%}<p class="bee-article-template__comment-warning">{{ 'blogs.comments.moderated' | t }}</p> {%- endif -%}
                                   
                                    <div class="bee-row bee-comment-form">
                                        <div class="bee-col-item bee-col-12">
                                            <div class="bee-form-group bee-pr">
                                                <textarea rows="5" name="comment[body]" id="CommentForm-body" class="bee-field__input {% if form_errors contains 'body' %} bee-input--error{% endif %}" aria-required="true" {% if form_errors contains 'body' %} aria-invalid="true" aria-describedby="CommentForm-body-error" {% endif %} placeholder="{{ message | t }}">{{ form.body }}</textarea>
                                                {%- if form_errors contains 'body' -%}<small id="CommentForm-body-error"><span class="bee-form__message">{% render 'icon-error' %}{{ message | t }} {{ form_errors_messages['body'] }}.</span></small>{%- endif -%}
                                            </div>
                                        </div>
                                        <div class="bee-col-item bee-col-md-6">
                                            <div class="bee-form-group bee-pr">
                                                <input type="text" name="comment[author]" id="CommentForm-author" class="bee-field__input {% if form_errors contains 'author' %}bee-input--error{% endif %}" autocomplete="name" value="{{ form.author }}" aria-required="true"{% if form_errors contains 'author' %}bee-input--error aria-invalid="true" aria-describedby="CommentForm-author-error"{% endif %} placeholder="{{ name | t }} *">
                                            </div>
                                            {%- if form_errors contains 'author' -%}<small id="CommentForm-author-error"><span class="bee-form__message">{% render 'icon-error' %}{{ name | t }} {{ form_errors_messages['author'] }}.</span></small>{%- endif -%}
                                        </div>
                                        <div class="bee-col-item bee-col-md-6">
                                            <div class="bee-form-group bee-pr">
                                                <input type="email" name="comment[email]" id="CommentForm-email" autocomplete="email" class="bee-field__input {% if form_errors contains 'email' %}bee-input--error{% endif %}" value="{{ form.email }}" autocorrect="off" autocapitalize="off" aria-required="true" {% if form_errors contains 'email' %} aria-invalid="true" aria-describedby="CommentForm-email-error"{% endif %} placeholder="{{ email | t }} *">
                                            </div>
                                            {%- if form_errors contains 'email' -%} <small id="CommentForm-email-error"><span class="bee-form__message">{% render 'icon-error' %}{{ email | t }} {{ form_errors_messages['email'] }}.</span></small>{%- endif -%}
                                        </div>
                                        
                                    </div>
                                    <div class="article-comments__submit">
                                        <input type="submit" class="bee-btn bee-btn-base bee-btn-style-{{ bk_stts.button_style }} bee-btn-size-{{ bk_stts.btn_size }} bee-btn-color-{{ bk_stts.btn_cl }} bee-btn-base bee-btn-style-{{ bk_stts.button_style }} bee-btn-size-{{ bk_stts.btn_size }} bee-btn-color-{{ bk_stts.btn_cl }}{% if bk_stts.enable_full_btn %} bee-w-100{% endif %} bee-btn-effect-fade bee-pe-auto" value="{{ 'blogs.comments.post' | t }}">
                                    </div>
                                {%- endform -%}
                            </div>
                        {%- when 'related' -%}
                            {{ 'bee-blogs.css' | asset_url | stylesheet_tag }}
                            {{ 'pre_flickitybee.min.css' | asset_url | stylesheet_tag }}
                            {{ 'bee-slider-settings.css' | asset_url | stylesheet_tag }}

                            {%-liquid 
                                assign index = 0 
                                assign b_effect = bk_stts.b_effect
                                assign img_effect = bk_stts.img_effect
                                if bk_stts.btn_owl == "outline"
                                    assign arrow_icon = 1
                                else
                                    assign arrow_icon = 2
                                endif
                                assign blog_url =  blog.url
                                assign blog_title =  blog.title
                                assign limit_related = bk_stts.limit_related

                                if bk_stts.btn_owl == "simple"
                                    assign arrow_icon = 1
                                else
                                    assign arrow_icon = 2
                                endif
                                assign show_btn = bk_stts.show_btn
                                if show_btn != "hidden"
                                    assign slider_btns = true
                                else
                                    assign slider_btns = false
                                endif
                                assign show_dots = bk_stts.show_dots
                                if show_dots != "hidden"
                                    assign slider_dots = true
                                else
                                    assign slider_dots = false
                                endif
                            -%} 
                            
                            <div class="bee-article-block bee-article-related" style="--mgb:{{ bk_stts.mgb }}px;--mgb-tb:{{ bk_stts.mgb_tb }}px;--mgb-mb:{{ bk_stts.mgb_mb }}px;">
                                <h4 class="bee-article-related__heading">{{ bk_stts.hd_related }}</h4>
                                {%- liquid
                                    assign icon_slider = bk_stts.icon_slider 
                                if icon_slider == "1"
                                    assign view_box = "0 0 22 22"
                                else icon_slider == "2"
                                    assign view_box = "0 0 22 22"
                                endif -%}
                                <div class="bee-flickity-slider bee-text-{{ bk_stts.text_align }} bee_{{ bk_stts.image_ratio }} bee_position_{{ bk_stts.image_position }} bee_{{ bk_stts.image_size }} flickitybee bee-row bee-row-cols-lg-{{ bk_stts.col_dk }} bee-row-cols-md-{{ bk_stts.col_tb }} bee-row-cols-{{ bk_stts.col_mb }} bee-gx-lg-{{ bk_stts.space_h_item }} bee-gy-lg-{{ bk_stts.space_v_item }} bee-gx-md-{{ bk_stts.space_h_item_tb }} bee-gy-md-{{ bk_stts.space_v_item_tb }} bee-gx-{{ bk_stts.space_h_item_mb }} bee-gy-{{ bk_stts.space_v_item_mb }} {% if slider_btns %} bee-slider-btn__{{ bk_stts.btn_pos }}-content bee-slider-btn-{{show_btn}} bee-slider-btn-style-{{ bk_stts.btn_owl }} bee-slider-btn-{{ bk_stts.btn_shape }} bee-slider-btn-{{ bk_stts.btn_size }} bee-slider-btn-cl-{{ bk_stts.btn_cl }} bee-slider-btn-vi-{{ bk_stts.btn_vi }} {% endif %}{% if slider_dots %} bee-dots-style-{{ bk_stts.dot_owl }} bee-slider-dots-{{show_dots}} bee-dots-cl-{{ bk_stts.dots_cl }} bee-dots-round-{{ bk_stts.dots_round }} {% endif %}" data-flickitybee-js='{"customIcon":{{icon_slider}}, "viewBox":"{{view_box}}", "setPrevNextButtons": true,"arrowIcon": "{{ arrow_icon }}","imagesLoaded": 0,"adaptiveHeight": 0, "contain": 1, "groupCells": "100%", "dragThreshold" : 5, "cellAlign": "left", "wrapAround": {{ bk_stts.loop }}, "prevNextButtons": {{ slider_btns }},"percentPosition": 1,"pageDots": {{ slider_dots }}, "autoPlay" : {{ bk_stts.au_time | times: 1000 }}, "pauseAutoPlayOnHover" : {{ bk_stts.au_hover }} }' style="--btn-distance: {{ bk_stts.btn_distance }}px;--space-dots: {{ bk_stts.dots_space }}px;--dots-bottom-pos: {{ bk_stts.dots_bottom_pos }}px;">
                                    {%- liquid
                                        for article in blog.articles 
                                            if article.id == article_id 
                                                continue  
                                            endif
                                            assign index = index | plus: 1
                                              render 'article_related' for blog.articles as article, article: article, blog_url: blog_url, blog_title: blog_title, b_effect: b_effect, img_effect: img_effect
                                            if index == limit_related
                                                break  
                                            endif 
                                        endfor
                                    -%}
                                </div>
                            </div>
                        
