<link href="{{ 'bee-base_drop.min.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">

{%- liquid
    assign sid = section.id
    assign se_stts = section.settings
    assign se_blocks = section.blocks
    assign stt_layout = se_stts.layout
    if stt_layout == 'bee-se-container' 
        assign html_layout = '<div class="bee-container">__</div></div>' | split: '__'
    elsif stt_layout == 'bee-container-wrap'
        assign html_layout = '<div class="bee-container">__</div>' | split: '__'
    else
        assign html_layout = '__' | split: '__'
    endif
    
    assign image_ratio = se_stts.image_ratio
    if image_ratio == "ratioadapt"
        assign imgatt = ''
    else 
        assign imgatt = 'data-'
    endif

    assign blog = blogs[se_stts.blog]
    assign show_author = se_stts.show_author
    assign show_comment = se_stts.show_comment
    assign show_tag = se_stts.show_tag
    assign show_readmore = se_stts.show_readmore
    assign show_date = se_stts.show_date
    assign date_format = se_stts.date
    assign by = 'blogs.article.by' | t
    assign blog_url = blog.url
    assign limit = se_stts.limit
    
    assign use_button = false
    assign b_effect = se_stts.b_effect
    assign img_effect = se_stts.img_effect

    assign text_cl = se_stts.text_cl

    assign ARRhtml1 = 'a,,' | split: ','
    assign ARRhtml2 = 'div,data-,data-' | split: ','

    assign no_block_parent = true

    if se_blocks.size > 0 and blog != blank
        assign item_col = 'bee-col-lg-6'
        assign chk_col = '2'
    else 
        assign item_col = 'bee-col-lg-12'
        assign chk_col = '1'
    endif 

    assign show_btn = se_stts.show_btn
    if show_btn != "hidden"
      assign slider_btns = true
    else
      assign slider_btns = false
    endif
     assign show_dots = se_stts.show_dots
    if show_dots != "hidden"
      assign slider_dots = true
    else
      assign slider_dots = false
    endif

    assign carousel_mobile = se_stts.carousel_mobile

    assign icon_slider = se_stts.icon_slider 
              if icon_slider == "1"
                assign view_box = "0 0 22 22"
              endif 
 -%}

              

{%- if carousel_mobile -%}
    {{ 'pre_flickitybee.min.css' | asset_url | stylesheet_tag }}
    {{ 'bee-slider-settings.css' | asset_url | stylesheet_tag }}
{%- endif -%}
<div class="bee-section-inner bee_nt_se_{{ sid }} bee_{{ image_ratio }} bee_se_{{ sid }} {{ stt_layout }}" {% render 'section_style', se_stts: se_stts -%}>  
    {{- html_layout[0] -}}
    {%- if stt_layout == 'bee-se-container' -%}<div class="bee-container-inner">{%- endif -%}
    <div class="bee-row bee-check-cols-{{ chk_col }} bee-pr bee-border-testi-{{ se_stts.border_item }} bee-blogs-testi bee-blogs-wrap bee-pos-ins-{{ se_stts.pos_ins }} bee-gy-lg-0 bee-gy-30">
        <div class="bee-col-item {{ item_col }} bee-col-12">
            {%- liquid
              assign icon_slider = se_stts.icon_slider 
              if icon_slider == "1"
                assign view_box = "0 0 22 22"
              endif -%}
            <div class="bee-blogs bee_position_{{ se_stts.image_position }} bee-blogs-height-{{ se_stts.cus_blog_heigt }} bee-gx-lg-0 bee-gy-lg-0 bee-gx-md-0 bee-gy-md-0 bee-gx-0 bee-gy-0 {% if carousel_mobile %}carousel-disable-lg bee-flickity-slider flickitybee bee-row bee-row-cols-1 bee-dots-style-elessi bee-dots-cl-dark bee-dots-round-true{% endif %}" data-flickitybee-js='{"watchCSS": 1,"setPrevNextButtons": false,"arrowIcon": "1","imagesLoaded": 0,"adaptiveHeight": 0, "contain": 1, "groupCells": "100%", "dragThreshold" : 5, "cellAlign": "left","wrapAround": true,"prevNextButtons": false,"percentPosition": 1,"pageDots": true, "autoPlay" : 2000, "pauseAutoPlayOnHover" : true }' style="--blog-max-height:{{ se_stts.blog_height }}px;--blog-max-height-tb:{{ se_stts.blog_height_tb }}px;--blog-max-height-mb:{{ se_stts.blog_height_mb }}px;--space-dots: 10px;">
                {%- if blog != blank %}
                    {%- for article in blog.articles limit: limit -%}
                        {%- assign art_url = article.url -%}
                        {%- assign image = article.image -%}
                        <div class="bee-article-item bee-pr bee-eff bee-eff-{{ b_effect }} bee-eff-img-{{ img_effect }}  bee-overlay-tb {% if carousel_mobile %}bee-col-item{% endif %}" style="--text-cl:{{ text_cl }}">   
                            <div class="bee-article-thumb bee-pr">                                          
                                <a class="bee-article-img bee-d-block" href="{{ art_url }}">
                                    <div class="bee_ratio bee-cus-height" style="--{{ imgatt }}aspect-ratioapt: {{ image.aspect_ratio | default: 1.7777 }}" data-cacl-slide>
                                        {%- if image != blank -%}
                                            <img class="lazyloadbee bee-lz--fadeIn bee-obj-eff" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg',w:image.width,h:image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}">
                                            <span class="lazyloadbee-loader is-bg-img" style="background: url({{ image | image_url: width: 1 }});"></span>
                                        {%- else -%}
                                            {{ 'image' | placeholder_svg_tag: 'bee-placeholder-svg bee-obj-eff' }}
                                        {%- endif -%} 
                                    </div>
                                </a>
                            </div>    
                            <div class="bee-article-info bee-pe-none bee-pa bee-l-0 bee-b-0 bee-r-0 bee-w-100 bee-article-info-alternative bee-text-center {% if show_readmore %} bee-has-readmore{% endif %}">
                                <div class="bee-article-metas bee-pe-auto">
                                    {%- if show_tag -%} 
                                        {%- if article.tags.size > 0 -%}
                                            <div class="bee-article-tags">
                                            {%- for tag in article.tags limit: 5 -%}
                                                <a href="{{ blog_url }}/tagged/{{ tag | handle }}">{{ tag | remove :'category_' | remove :'blog_' | strip }}</a>{%- unless forloop.last -%} <span class="bee-seperate"></span> {% endunless -%}
                                            {%- endfor -%}
                                            </div>
                                        {%- endif -%}
                                    {%- endif -%}
                                
                                </div>
                                <h3 class="bee-article-title bee-pe-auto"><a href="{{ art_url }}">{{ article.title }}</a></h3>
                                {%- if show_date -%}<div class="bee-article-time"><time class="bee-entry-date">{%- if date_format contains "%" -%}{{ article.published_at | time_tag: date_format }}{%- else -%}{{ article.published_at | time_tag: format: date_format }}{%- endif -%}</time></div>{%- endif -%}
                            
                            </div>                            
                        </div>
                    {%- endfor -%}
                
                {%- endif -%}
            </div>
        </div>
    <!-- render testimonial -->
        {%- if se_blocks.size > 0 -%}
        <div class="bee-col-item bee-testimonials {{ item_col }} bee-col-12" style="--pd-head-lr: 0px;--head-mgb: {{ se_stts.tophead_mb }}px;--head-mgb-tb: {{ se_stts.tophead_mb_tb }}px;--head-mgb-mb: {{ se_stts.tophead_mb_mb }}px;--cl-name: {{ se_stts.cl_name }};--cl-content: {{ se_stts.cl_content }};--bd-cl: {{ se_stts.cl_bd }};--bg-cl: {{ se_stts.cl_bg_item }};--rating-cl: {{ se_stts.rating_cl }};--position-cl:{{ se_stts.position_cl }}">
