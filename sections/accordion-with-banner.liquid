<!-- sections/accordion.liquid -->
{{ 'section.css' | asset_url | stylesheet_tag }}
{{ 'bee-section.css' | asset_url | stylesheet_tag }}
{{ 'bee-content-position.css' | asset_url | stylesheet_tag }}
{{ 'bee-banner.css' | asset_url | stylesheet_tag }}
{{ 'bee-accordion.css' | asset_url | stylesheet_tag }}
<link href="{{ 'bee-animation.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
<link href="{{ 'bee-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
	  assign sid = section.id
	  assign se_stts = section.settings
	  assign se_blocks = section.blocks
	  assign stt_layout = se_stts.layout
	  assign stt_image_bg = se_stts.image_bg
    assign countdown = false
    assign general_block = false
    assign isFirstAccordion = se_stts.auto_active
    assign isFirstAccordion2 = se_stts.auto_active
	  if stt_layout == 'bee-se-container' 
	    assign html_layout = '<div class="bee-container">__</div></div>' | split: '__'
	  elsif stt_layout == 'bee-container-wrap'
	    assign html_layout = '<div class="bee-container">__</div>' | split: '__'
	  else
	    assign html_layout = '__' | split: '__'
	  endif

    assign block_type = block.type
    if block_type == 'anner_accordion'
        assign banner =  '<div class="bee-row">__</div>' | split: '__'
    endif
    assign no_block_parent = true
 -%}
  {%- capture append_style -%}
      --aspect-ratio-cus : {{ se_stts.height_dk }}px;--aspect-ratio-custb : {{ se_stts.height_tb }}px;--aspect-ratio-cusmb : {{ se_stts.height_mb }}px;
  {%- endcapture -%}
  {%- style -%}
  .bee-tab-wrapper:first-child .bee-blank-des.bee-tab-content:not([style]),
    .bee-a_wbanner .bee-tab-wrapper .bee-blank-des{
      display: none;
    }
  {%- endstyle -%}
  <div class="bee-section-inner bee_se_{{ sid }} {{ stt_layout }}{% if stt_image_bg != blank and stt_layout != 'bee-se-container' %} bee-has-imgbg lazyloadbee{% endif %}" {%- if stt_image_bg != blank and stt_layout != 'bee-se-container' -%} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5"{%- endif -%} {% render 'section_style', se_stts: se_stts %}>
      {{- html_layout[0] | replace: '010693', se_stts.custom_width -}}
      {%- if stt_layout == 'bee-se-container' -%}<div class="bee-container-inner {% if stt_image_bg != blank %} bee-has-imgbg lazyloadbee {% endif %} "  {%- if stt_image_bg != blank -%} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5" {%- endif -%} > {%- endif -%}
       {%- render 'section_tophead', se_stts: se_stts, get_height: true -%}
      <div class="bee-row bee-accordion-banner bee-gx-md-{{ se_stts.space_h_item_tb }} bee-gy-md-{{ se_stts.space_v_item_tb }} bee-gx-{{ se_stts.space_h_item_mb }} bee-gy-{{ se_stts.space_v_item_mb }}">

        {%- for block in se_blocks %}{% assign block_type = block.type -%}
            {%- if block_type contains '_parent' -%} 

              {%- liquid
                  unless no_block_parent
                      echo close_div_tag
                  endunless
                  assign no_block_parent = false       
                  assign bk_stts = block.settings
                  assign isParentBanner = false
                  assign isParentAccordion = false  -%}

              {%- if block_type == 'accordion_parent' -%}

                  {%-liquid
                  assign close_div_tag = '</div></div></div>'
                  assign isParentAccordion = true -%}
                  <div class="bee-col-item bee-col-xl-4 bee-col-lg-{{ bk_stts.col_dk }} bee-col-md-{{ bk_stts.col_tb }} bee-col-{{ bk_stts.col_mb }}" {% if bk_stts.col_dk == 'custom' or bk_stts.col_tb == 'custom' %}style="--bee-cus-col-lg:{{ bk_stts.col_dk_w | remove: ' ' }}%;--bee-cus-col-md:{{bk_stts.col_dk_w_tb | remove: ' ' }}%;"{% endif %}>
                    <div class="bee-accordion-parent bee-accordion-style-2" style="--fs-content: {{bk_stts.fs_content}}px; --fw-content: {{bk_stts.fw_content}}; --title-pd: {{bk_stts.title_pd}}; --fs-title: {{bk_stts.fs_title}}px; --fw_title: {{bk_stts.fw_title}}">
                        <div class="bee-tabs bee-type-accordion {%- if bk_stts.border_bt %} bee_border_bottom {%- endif %} bee-text-{{ bk_stts.content_align }}" data-bee-tabs style="--title-cl: {{ bk_stts.title_cl }};--bg-title-cl: {{ bk_stts.bg_title_cl }};--title-active-cl: {{ bk_stts.title_active_cl }};--bg-title-active-cl: {{ bk_stts.bg_title_active_cl }};--content-cl: {{bk_stts.content_cl}};--bg-content-cl: {{bk_stts.bg_ac_content_cl}}">
                           
                    
              {%- else -%}

                  {%- liquid 
                    assign close_div_tag = '</div></div></div></div>'
                    assign isParentBanner = true      
                    assign bk_stts = block.settings
                    assign image_mb = bk_stts.image_mb
                    assign image = bk_stts.image | default: image_mb
                    assign image_mb = image_mb | default: image
                    assign b_link = bk_stts.b_link
                    assign ani_delay = 0   
                    assign percent_delay = bk_stts.animation_delay | divided_by: 100.0
                    assign time_ani_delay = bk_stts.time_animation | times: percent_delay  
                  -%}
                  <div class="bee-col-item bee-banner-wrap bee_{{ bk_stts.image_size }} bee-col-lg-{{ bk_stts.col_dk }} bee-col-md-{{ bk_stts.col_tb }} bee-col-{{ bk_stts.col_mb }} bl-{{ block.id }}" {% if bk_stts.col_dk == 'custom' or bk_stts.col_tb == 'custom' %}style="--bee-cus-col-lg:{{ bk_stts.col_dk_w | remove: ' ' }}%;--bee-cus-col-md:{{bk_stts.col_dk_w_tb | remove: ' ' }}%;"{% endif %}>
                    <div data-bee-animate class="bee-banner-item bee-eff bee-eff-{{ bk_stts.b_effect }} bee-eff-img-{{ bk_stts.img_effect }} bee_ratio_mix bee_ratio_cuspx_mb_{{ bk_stts.custom_mb }} bee_ratio_cuspx_tb_{{ bk_stts.custom_tb }} bee_ratio_cuspx_{{ bk_stts.custom_dk }}" style="--ratioapt: {{ image.aspect_ratio | default: 1.5 }};--ratioaptmb: {{ mb_image.aspect_ratio | default: 1.5 }};--aspect-ratio-cus : {{ bk_stts.height_dk }}px;--aspect-ratio-custb : {{ bk_stts.height_tb }}px;--aspect-ratio-cusmb : {{ bk_stts.height_mb }}px;">
                      <div class="bee-banner-inner">
                        {%- liquid 
                          if b_link != blank 
                            assign ARRhtml = 'a,,' | split: ','
                          else
                            assign ARRhtml = 'div,data-,data-' | split: ','
                          endif
                        -%}

                        {%- if bk_stts.bg_content_bl -%}
                          {%- capture append_bg_content_style -%}
                            --content-pd: {{ bk_stts.content_pd_tb }}px {{ bk_stts.content_pd_lr }}px;--content-pd-mb: {{ bk_stts.content_pd_tb_mb }}px {{ bk_stts.content_pd_lr_mb }}px;
                          {%- endcapture -%}
                        {%- endif -%}
                        {%- if bk_stts.border_bl -%}
                          {%- capture append_bg_border_style -%}
                            --br-color: {{ bk_stts.br_color }};--br-style: {{ bk_stts.br_style }};--br-pd: {{ bk_stts.br_pd }}px;--br-pd-mb: {{ bk_stts.br_pd_mb }}px;
                          {%- endcapture -%}
                        {%- endif -%}

                        <{{ ARRhtml[0] }} {{ ARRhtml[1] }}href="{{ bk_stts.b_link }}"  {{ ARRhtml[2] }}target="{{ bk_stts.open_link }}" class="bee-d-block bee_ratio bee_ratio_hasmb bee-bg-pos-{{ bk_stts.image_position }}">
                          {%- if image != blank -%}
                              <img {% if image_mb.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{image_mb.presentation.focal_point}}"{% endif %} class="lazyloadbee bee-lz--fadeIn bee-obj-eff bee-d-md-none" data-src="{{ image_mb | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg',w: image_mb.width,h: image_mb.height %}" width="{{ image_mb.width }}" height="{{ image_mb.height }}" alt="{{ image_mb.alt | escape }}">
                              <span class="lazyloadbee-loader is-bg-img bee-d-md-none" style="background: url({{ image_mb | image_url: width: 1 }})"></span>
                              <img {% if image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{image.presentation.focal_point}}"{% endif %} class="lazyloadbee bee-lz--fadeIn bee-obj-eff bee-d-none bee-d-md-block" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg', w: image.width, h: image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}">
                              <span class="lazyloadbee-loader is-bg-img bee-d-none bee-d-md-block" style="background: url({{ image | image_url: width: 1 }})"></span>
                          {%- else -%}
                            {{ 'image' | placeholder_svg_tag: 'bee-placeholder-svg bee-svg-bg1 bee-obj-eff' }}
                          {%- endif -%}
                        </{{ ARRhtml[0] }}>
                        <div class="bee-banner-content bee-content-position bee-pa {{ bk_stts.content_width }} bee-text-lg-{{ bk_stts.content_align }} bee-text-md-{{ bk_stts.content_align_tb }} bee-text-{{ bk_stts.content_align_mb }} bee-bg-content-{{ bk_stts.bg_content_bl }} bee-br-content-{{ bk_stts.border_bl }} bee-br-style-{{ bk_stts.br_style }}" style="--time-animation: {{ bk_stts.time_animation }}s; {%- render 'content_position',ch_pos: bk_stts.ch_pos,cv_pos: bk_stts.cv_pos,ch_pos_tb: bk_stts.ch_pos_tb,cv_pos_tb: bk_stts.cv_pos_tb,ch_pos_mb: bk_stts.ch_pos_mb,cv_pos_mb: bk_stts.cv_pos_mb,append_bg_content_style: append_bg_content_style,append_bg_border_style: append_bg_border_style -%}">
                      
                {%- endif -%}

            {%- else -%}
  
                  {%- liquid
                  if no_block_parent
                    continue
                  endif
                  assign bk_stts = block.settings
                  if isParentAccordion and block_type != 'accor_item'
                    continue
                  elsif isParentBanner and block_type == 'accor_item' 
                    continue
                  endif -%}
                  {%- case block_type -%}
                  {%- when 'custom_text' -%}
                    {% if bk_stts.text != blank %}
                      {%- assign general_block = true -%}
                      <{{ bk_stts.tag }} data-lh="{{bk_stts.text_lh_mb}}" data-lh-md="{{bk_stts.text_lh_tb}}" data-lh-lg="{{bk_stts.text_lh}}" class="bee-bl-item bee-pe-none bee-animation-{{ bk_stts.animation }} bee-text-bl bee-fnt-fm-{{ bk_stts.fontf }} bee-font-italic-{{ bk_stts.font_italic }} bee-hidden-mobile-{{ bk_stts.hidden_mobile }} bee-hidden-br-mb-{{ bk_stts.remove_br_tag }} bee-text-shadow-{{ bk_stts.text_shadow }}" id="b_{{ block.id }}" {%- render 'bk_cus_style',type: 'custom_text', bk_stts: bk_stts,ani_delay: ani_delay -%}>{{ bk_stts.text }}</{{ bk_stts.tag }}>
                    {% endif %}
                  {%- when 'space_html' -%}
                    {%- assign general_block = true -%}
                    <div class="bee-space-html bee-bl-item bee-animation-{{ bk_stts.animation }} bee-hidden-mobile-{{ bk_stts.hidden_mobile }}" {%- render 'bk_cus_style',type: 'space_html', bk_stts: bk_stts,ani_delay: ani_delay -%}></div>
                  {%- when 'html' -%}
                    {% if bk_stts.html_content != blank %}
                      {%- assign general_block = true -%}
                        <div class="bee-bl-item bee-animation-{{ bk_stts.animation }} bee-raw-html bee-hidden-mobile-{{ bk_stts.hidden_mobile }} bee-rte--list" id="b_{{ block.id }}" {%- render 'bk_cus_style',type: 'html', bk_stts: bk_stts,ani_delay: ani_delay -%}>{{ bk_stts.html_content }}</div>
                      {% endif %}
                  {%- when 'image' -%}
                    {%- assign image = bk_stts.image_child -%}
                    {%- if bk_stts.img_link != blank -%}
                      {%- assign image_parent = 'a,' | split: ',' -%}
                    {%- else -%}
                      {%- assign image_parent = 'div,data-' | split: ',' -%}
                    {%- endif -%}
                    {%- if image != blank -%}
                        {%- assign general_block = true -%}
                        {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                        <{{ image_parent[0] }} {{ image_parent[1] }}href="{{ bk_stts.img_link }}" class="bee-bl-item bee-img-child bee-animation-{{ bk_stts.animation }} bee-hidden-mobile-{{ bk_stts.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style',type: 'image', bk_stts: bk_stts,ani_delay: ani_delay -%}>
                            <img class="lazyloadbee bee-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg', w: image.width, h: image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}">
                        </{{-image_parent[0]-}}>
                    {%- else -%}
                      {{ 'image' | placeholder_svg_tag: 'bee-placeholder-svg bee-svg-bg1 bee-obj-eff' }}
                    {%- endif -%}
                  {%- when "countdown" -%}
                    {%- if bk_stts.date != blank -%}
                    {%- assign countdown = true -%}
                      <div class="bee-bl-item bee-animation-{{ bk_stts.animation }} bee-countdown sepr_coun_dt_wrap bee-countdown-des-{{ bk_stts.cdt_des }} bee-countdown-size-{{ bk_stts.cdt_size }} bee-hidden-mobile-{{ bk_stts.hidden_mobile }}" id="b_{{ block.id }}" {%- render 'bk_cus_style',type: 'countdown', bk_stts: bk_stts,ani_delay: ani_delay -%}>
                        <div class="bee-time" data-countdown-bee data-date='{{ bk_stts.date }}' data-keyid='#countdown-{{ sid }}'></div>
                      </div>
                    {% endif %}
                  {%- when 'custom_button' -%}
                    {%- if bk_stts.button_link != blank and bk_stts.button_text != blank -%}
                      {%- assign custom_button = true -%}
                      {%- assign  button_style = bk_stts.button_style -%}
                      <a href="{{ bk_stts.button_link }}" target="{{ bk_stts.target_link }}" class="bee-bl-item bee-animation-{{ bk_stts.animation }} bee-btn bee-btn-custom  bee-pe-auto bee-fnt-fm-{{ bk_stts.fontf }} bee-hidden-mobile-{{ bk_stts.hidden_mobile }} bee-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline' %}bee-btn-effect-{{ bk_stts.button_effect }}{% endif %}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style',type: 'custom_button', bk_stts: bk_stts,ani_delay: ani_delay -%}>{{ bk_stts.button_text }}
                        <?xml version="1.0" encoding="iso-8859-1"?>
                        {%- if button_style == 'wave_bottom' -%}
                        <svg class="bee-btn-underline" version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="172px" height="5px" viewBox="0 0 172 5" style="enable-background:new 0 0 172 5;" xml:space="preserve"><g><g><path d="M-1001,250c-0.17,0-0.35-0.05-0.51-0.14l-4.49-2.69l-4.49,2.69c-0.32,0.19-0.71,0.19-1.03,0l-4.49-2.69l-4.49,2.69 c-0.32,0.19-0.71,0.19-1.03,0l-4.49-2.69l-4.49,2.69c-0.32,0.19-0.71,0.19-1.03,0l-4.49-2.69l-4.49,2.69      c-0.32,0.19-0.71,0.19-1.03,0l-4.49-2.69l-4.49,2.69c-0.32,0.19-0.71,0.19-1.03,0l-4.49-2.69l-4.49,2.69 c-0.32,0.19-0.71,0.19-1.03,0l-4.49-2.69l-4.49,2.69c-0.47,0.28-1.09,0.13-1.37-0.34c-0.28-0.47-0.13-1.09,0.34-1.37l5-3  c0.32-0.19,0.71-0.19,1.03,0l4.49,2.69l4.49-2.69c0.32-0.19,0.71-0.19,1.03,0l4.49,2.69l4.49-2.69c0.32-0.19,0.71-0.19,1.03,0 l4.49,2.69l4.49-2.69c0.32-0.19,0.71-0.19,1.03,0l4.49,2.69l4.49-2.69c0.32-0.19,0.71-0.19,1.03,0l4.49,2.69l4.49-2.69
                        c0.32-0.19,0.71-0.19,1.03,0l4.49,2.69l4.49-2.69c0.32-0.19,0.71-0.19,1.03,0l5,3c0.47,0.28,0.63,0.9,0.34,1.37
                        C-1000.33,249.83-1000.66,250-1001,250z M71,5c-0.17,0-0.35-0.05-0.51-0.14L66,2.17l-4.49,2.69c-0.32,0.19-0.71,0.19-1.03,0
                        L56,2.17l-4.49,2.69c-0.32,0.19-0.71,0.19-1.03,0L46,2.17l-4.49,2.69c-0.32,0.19-0.71,0.19-1.03,0L36,2.17l-4.49,2.69
                        c-0.32,0.19-0.71,0.19-1.03,0L26,2.17l-4.49,2.69c-0.32,0.19-0.71,0.19-1.03,0L16,2.17l-4.49,2.69c-0.32,0.19-0.71,0.19-1.03,0
                        L6,2.17L1.51,4.86C1.04,5.14,0.43,4.99,0.14,4.51S0.01,3.43,0.49,3.14l5-3c0.32-0.19,0.71-0.19,1.03,0L11,2.83l4.49-2.69 c0.32-0.19,0.71-0.19,1.03,0L21,2.83l4.49-2.69c0.32-0.19,0.71-0.19,1.03,0L31,2.83l4.49-2.69c0.32-0.19,0.71-0.19,1.03,0L41,2.83 l4.49-2.69c0.32-0.19,0.71-0.19,1.03,0L51,2.83l4.49-2.69c0.32-0.19,0.71-0.19,1.03,0L61,2.83l4.49-2.69 c0.32-0.19,0.71-0.19,1.03,0l5,3c0.47,0.28,0.63,0.9,0.34,1.37C71.67,4.83,71.34,5,71,5z"/></g></g></svg>
                        {%- endif -%}
                        {%- if bk_stts.button_icon_w > 0 -%}
                          {%- if bk_stts.icon_style == "1" -%}
                            <svg  class="bee-btn-icon" viewBox="0 0 22 22"><use xlink:href="#bee-icon-btn"></use></svg>
                          {%- else -%}
                            <svg class="bee-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M96 480c-8.188 0-16.38-3.125-22.62-9.375c-12.5-12.5-12.5-32.75 0-45.25L242.8 256L73.38 86.63c-12.5-12.5-12.5-32.75 0-45.25s32.75-12.5 45.25 0l192 192c12.5 12.5 12.5 32.75 0 45.25l-192 192C112.4 476.9 104.2 480 96 480z"/></svg>
                          {%- endif -%}
                        {%- endif -%}
                     </a>
                    {%- endif -%}
                  {%- when 'newsletter' -%}
                    {%- assign newsletter = true -%}
                    {%- assign custom_button = true -%}
                    <div id="b_{{ block.id }}" class="bee-bl-item bee-animation-{{ bk_stts.animation }} bee-newsletter-parent bee-custom-width-{{ bk_stts.custom_width }} bee_newsletter_se bee-newsl-des-{{ bk_stts.newl_des }} bee-newsl-{{ bk_stts.newl_size }} bee-hidden-mobile-{{ bk_stts.hidden_mobile }} bee-has-btn-{{ bk_stts.source_btn }}" {%- render 'bk_cus_style',type: 'newsletter', bk_stts: bk_stts,ani_delay: ani_delay -%}>
                      {%- render 'newsletter', layout: bk_stts.newl_des, form_id: block.id, source_btn: bk_stts.source_btn, buttonIcon: bk_stts.news_icon -%}
                    </div>
                  {%- when 'accor_item' %}{% assign blockid = block.id -%}
                        <div  class="bee-tab-wrapper{% if isFirstAccordion %} bee-active{% assign isFirstAccordion = false %}{% endif %}" data-bee-tab-wrapper {{ block.shopify_attributes }}>
                         <a id="b_{{ blockid }}" class="bee-accor-title" href="#bee-{{ blockid }}" rel="nofollow" data-bee-tab-item data-no-instant>
                            <span class="bee-accor-text">
                              {{ bk_stts.title }}
                            </span>
                            <span class="bee-accor-item-nav"> </span>
                          </a>  
                          <div id="bee-{{ blockid }}" class="bee-tab-content {% if isFirstAccordion2 == false %} bee-blank-des{% endif %} bee-rte" data-bee-tab-content>{{ bk_stts.content }} </div>
                        </div>
                {%- endcase -%}

            {%- endif -%}
                  
            {%- liquid 
            if forloop.last
            echo close_div_tag
            endif -%}

        {%- endfor -%}
      </div>
    {{- html_layout[1] -}}
  </div>

      {%- if general_block or custom_button or newsletter -%}
        {{ 'bee-general-block.css' | asset_url | stylesheet_tag }}
      {%- endif -%}
      {%- if custom_button -%}
        {{ 'bee-button-style.css' | asset_url | stylesheet_tag }}
      {%- endif -%}
      {%- if newsletter -%}
        {{ 'newsletter.css' | asset_url | stylesheet_tag }}
      {%- endif -%}
      {%- if countdown -%} 
        {{ 'bee-countdown.css' | asset_url | stylesheet_tag }}
        <template id="countdown-{{ sid }}">
            <span class="countdown-days">
                <span class="cd_timebee cd-number">%-D</span>
                <span class="cd_txtbee cd-text">%!D:{{ "sections.countdown_text.day" | t }},{{ "sections.countdown_text.day_plural" | t }};</span>
            </span>
            <span class="countdown-hours"> 
                <span class="cd_timebee cd-number">%H</span> 
                <span class="cd_txtbee cd-text">%!H:{{ "sections.countdown_text.hr" | t }},{{ "sections.countdown_text.hr_plural" | t }};</span>
            </span>
            <span class="countdown-min">
                <span class="cd_timebee cd-number">%M</span> 
