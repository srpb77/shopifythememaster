{%- liquid
  assign image_fix = image_nt | image_tag
  assign sid = section.id
  assign se_stts = section.settings
  assign se_blocks = section.blocks
  assign stt_layout = se_stts.layout
  assign stt_image_bg = se_stts.image_bg
  assign slide_eff = se_stts.eff
  assign se_height = se_stts.se_height
  if stt_layout == 'bee-se-container' 
    assign html_layout = '<div class="bee-container">__</div></div>' | split: '__'
  elsif stt_layout == 'bee-container-wrap'
    assign html_layout = '<div class="bee-container">__</div>' | split: '__'
  else
    assign html_layout = '__' | split: '__'
  endif
  assign index = 0
  assign index2 = 1
  if settings.use_mytillang_img and request.locale.primary == false
        assign use_mytillang_img = true
        assign img_iso_code = request.locale.iso_code | prepend: '_' | append: '.'
    else
        assign use_mytillang_img = false
    endif 
  assign isRunLoop = true
    assign SetBL = true
    if se_height == 'bee_ratioadapt_mix'
        assign attr_img = ''
    elsif se_height == 'bee_ratioadapt_f_mix'
        assign attr_img = ''
        assign SetBL = false
    else 
        assign attr_img = 'data-'
    endif 
  if se_stts.btn_owl == "outline"
    assign arrow_icon = 1
  else
    assign arrow_icon = 2
  endif 
 
  assign use_animation = false
  assign no_block_parent = true
  assign start_slide     = true
  assign end_slide       = false

  assign show_btn = se_stts.show_btn
  if show_btn != "hidden"
    assign slider_btns = true
  else
    assign slider_btns = false
  endif
  assign show_dots = se_stts.show_dots
  if show_dots != "hidden"
    assign slider_dots = true
  else
    assign slider_dots = false
  endif
  assign nav_btn_icon = se_stts.nav_btn_icon
  assign navUI = false
  if show_btn != "hidden" and nav_btn_icon != "default" 
      assign navUI = true
  endif 
  if show_btn != "hidden" and se_stts.btn_pos == "vertical"
    assign navUI = true
  endif     
  assign unit_dots_vertical = se_stts.unit_dots_vertical
  assign unit_nav_vertical = se_stts.unit_nav_vertical
 -%} 
