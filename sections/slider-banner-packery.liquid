                                    {%- endif -%}
                                {%- endif -%}
                            </a>
                        {%- endif -%}

                {%- endcase -%}
                {%- if bk_stts.animation != 'none' %}{% assign ani_delay = ani_delay | plus: time_ani_delay %}{% endif -%}

            {%- endif -%}

            {%- liquid
            if forloop.last
               echo close_div_tag
               if end_slide
                 echo '</div></div></div><!-- end slider s-->'
               endif
            endif -%}
        {%- endfor -%}
