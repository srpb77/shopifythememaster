        {%- endif -%}
        {%- if show_btn != "hidden" and se_stts.btn_pos == "vertical" -%}
          <div class="bee-flicky-btn__custom bee-flicky-btn__{{ se_stts.btn_pos }} bee-flickity-btn__{{ se_stts.btn_align }} bee-flickity-slider bee-slider-btn-{{ se_stts.btn_pos }} bee-slider-btn-style-{{ se_stts.btn_owl }} bee-slider-btn-{{ se_stts.btn_shape }} bee-slider-btn-{{ se_stts.btn_size }} bee-slider-btn-cl-{{ se_stts.btn_cl }} bee-slider-btn-vi-{{ se_stts.btn_vi }} bee-slider-btn-hidden-mobile-{{ se_stts.btn_hidden_mobile }}" style="--space-btn: {{ se_stts.nav_space }}px;--nav-position-vertical: {{ se_stts.nav_pos_vy }}{{ unit_nav_vertical }};{%- if unit_nav_vertical == '%' -%}--nav-transform-vertical: {{ se_stts.nav_pos_vy }}%;{%- endif -%}--slideshow-btn-pos: {{ se_stts.btn_distance }}px;">
                <button class="flickitybee-button flickitybee-prev-next-button previous bee-d-inline-flex btn__prev--{{ sid }}" type="button" aria-label="Previous">
                    {%- case nav_btn_icon -%}
                        {%- when "1" -%}
                            <svg class="flickitybee-button-icon" version="1.1" x="0px" y="0px" width="22px" height="22px" viewBox="0 0 22 22" xml:space="preserve">
                               <polygon points="16,2 16,20 5,11 "></polygon>
                            </svg>
                        {%- when "default" -%}
                          <svg class="flickitybee-button-icon" x="0px" y="0px" viewBox="0 0 22 22" xml:space="preserve"><g><path d="M14,18c-0.26,0-0.51-0.1-0.71-0.29L6.59,11l6.71-6.71c0.39-0.39,1.02-0.39,1.41,0s0.39,1.02,0,1.41L9.41,11l5.29,5.29c0.39,0.39,0.39,1.02,0,1.41C14.51,17.9,14.26,18,14,18z"/></g></svg>
                    {%- endcase -%}
                </button>
                <button class="flickitybee-button flickitybee-prev-next-button next bee-d-inline-flex btn__next--{{ sid }}" type="button" aria-label="Next">
                    {%- case nav_btn_icon -%}
                        {%- when "1" -%}
                            <svg class="flickitybee-button-icon" version="1.1" x="0px" y="0px" width="22px" height="22px" viewBox="0 0 22 22" xml:space="preserve">
                                <polygon points="6,20 6,2 17,11 "></polygon>
                            </svg>
                        {%- when "default" -%}
                            <svg class="flickitybee-button-icon" x="0px" y="0px" viewBox="0 0 22 22" xml:space="preserve"><g><path d="M8,18c-0.26,0-0.51-0.1-0.71-0.29c-0.39-0.39-0.39-1.02,0-1.41L12.59,11L7.29,5.71c-0.39-0.39-0.39-1.02,0-1.41s1.02-0.39,1.41,0L15.41,11l-6.71,6.71C8.51,17.9,8.26,18,8,18z"/></g></svg>
                        {%- endcase -%}
                </button>  
            </div>
        {%- endif -%}
        {%- if se_stts.enable_slide_number -%}   
            <div class="bee-slide-numbers bee-pe-none carousel--status{{ sid }} bee-dots-cl-{{ se_stts.dots_cl }}" style="--dots-num-space: {{ se_stts.dots_num_space }}px;--dots-num-position-vertical: {{ se_stts.dots_num_pos_vy }}{{ se_stts.unit_dots_num_vertical }};{%- if se_stts.unit_dots_num_vertical == '%' -%}--dots-num-transform-vertical: {{ se_stts.dots_num_pos_vy }}%;{%- endif -%}">
                <span class="bee-slide-number__current" data-current-slide></span>
                <span class="bee-slide-number__length" data-total-number></span>
                <p class="bee-slide-number__text">{{ se_stts.text_slide_number }}</p>
            </div>
        {%- endif -%}
        {%- if se_stts.use_socials -%}
          {{ 'bee-icon-social.css' | asset_url | stylesheet_tag }}
          {% liquid 
            assign pri_cl_lightness       = se_stts.pri_cl | color_extract: 'lightness'
            assign pri_hover_cl_lightness       = se_stts.pri_hover_cl | color_extract: 'lightness'
            if pri_cl_lightness < 85
              assign pri_cl2 = "#fff"
            else 
              assign pri_cl2 = "#000"
            endif
            if pri_hover_cl_lightness < 85
              assign pri_hover_cl2 = "#fff"
            else 
              assign pri_hover_cl2 = "#000"
            endif
          -%}  
          <div class="bee-socials-block bee-socials-align-{{ se_stts.socials_align }} bee-visible-{{ se_stts.socials_visibale }}" style="--pri-cl: {{ se_stts.pri_cl }};--pri-cl2: {{ pri_cl2 }};--pri-hover-cl: {{ se_stts.pri_hover_cl }};--pri-hover-cl2: {{ pri_hover_cl2 }};--bd-radius: {{ se_stts.bd_radius }}px;--socials-space: {{ se_stts.socials_space }}px;--socials-position-vertical: {{ se_stts.socials_pos_vy }}{{ se_stts.unit_socials_vertical }};{%- if se_stts.unit_socials_vertical == '%' -%}--socials-transform-vertical: {{ se_stts.socials_pos_vy }}%;{%- endif -%}">
            <div class="bee-list-socials">
             {%- if se_stts.social_mode == '1' -%} 
               {%- assign follow_social = true -%} 
             {%- else -%} 
               {%- assign share_image = settings.share_image | default: page_image | default: settings.logo -%} 
             {%- endif -%} 
              {%- render 'social_sharing',style: se_stts.social_style,use_color_set: se_stts.use_color_set,size: se_stts.social_size,space_h_item: se_stts.space_item, space_v_item: se_stts.space_item, space_v_item_mb: se_stts.space_item, space_h_item_mb: se_stts.space_item, share_image: share_image, follow_social: follow_social -%} 
            </div> 
          </div>
        {%- endif -%}  
    </div>  
    {{- html_layout[1] -}} 
