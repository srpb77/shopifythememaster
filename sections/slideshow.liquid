            <div class="bee-flicky-btn__custom bee-flickity-slider bee-slider-btn-{{ se_stts.btn_pos }} bee-slider-btn-style-{{ se_stts.btn_owl }} bee-slider-btn-{{ se_stts.btn_shape }} bee-slider-btn-{{ se_stts.btn_size }} bee-slider-btn-cl-{{ se_stts.btn_cl }} bee-slider-btn-vi-{{ se_stts.btn_vi }} bee-slider-btn-hidden-mobile-{{ se_stts.btn_hidden_mobile }}" style="--slideshow-btn-pos: {{ se_stts.btn_distance }}px;">
                <button class="flickitybee-button flickitybee-prev-next-button previous bee-d-inline-flex btn__prev--{{ sid }}" type="button" aria-label="Previous">
