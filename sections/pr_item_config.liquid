				        when '3'
				          render 'pr-grid-item3' for collections.all.products as product, product: product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, placeholder_img: placeholder_img, show_vendor: false, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, imgatt: imgatt, show_list_btns: false
				        when '4'
				          render 'pr-grid-item4' for collections.all.products as product, product: product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, placeholder_img: placeholder_img, show_vendor: false, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, imgatt: imgatt, show_list_btns: false
				        when '5'
				          render 'pr-grid-item5' for collections.all.products as product, product: product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, placeholder_img: placeholder_img, show_vendor: false, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, imgatt: imgatt, show_list_btns: false
				        when 'list'
				          render 'pr-grid-item1' for collections.all.products as product, product: product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, show_vendor: false, placeholder_img: placeholder_img, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, imgatt: imgatt, show_list_btns: true
				        when 'packery'
				          render 'pr-packery-item' for collections.all.products as product, product: product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, show_vendor: false, placeholder_img: placeholder_img, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, imgatt: imgatt, show_list_btns: false
				     endcase -%}
				</div> 
			{% endfor %} 
		{%- endpaginate -%}
	</div>
</div>
{%- endif -%}

