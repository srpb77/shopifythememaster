				</div> 
				<div class="bee_box_pr_grid bee-products {% if block.type == "list" %} is--listview {% endif %} bee_ratio1_1 bee_rationt bee_position_8 bee_cover bee-row bee-row-cols-lg-4 bee-row-cols-md-2 bee-row-cols-2 bee-gx-md-30 bee-gy-md-0 bee-gx-10 bee-gy-30 bee_mb_100" style="margin-bottom: 50px;">
				{%- liquid 	
				    case block.type
				        when '1'
				          render 'pr-grid-item1' for collections.all.products as product, product: product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, placeholder_img: placeholder_img, show_vendor: false, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, imgatt: imgatt, show_list_btns: false
